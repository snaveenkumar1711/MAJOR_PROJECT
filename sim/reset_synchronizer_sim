T_0.2 ;
    %load/vec4 v0x55d8db4bb0c0_0;
    %cmpi/s 2, 0, 32;
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x55d8db46f260_0;
    %pushi/vec4 1, 0, 34;
    %load/vec4 v0x55d8db4bb0c0_0;
    %subi 1, 0, 32;
    %pad/s 34;
    %sub;
    %part/s 1;
    %ix/load 5, 0, 0;
    %pushi/vec4 1, 0, 34;
    %load/vec4 v0x55d8db4bb0c0_0;
    %pad/s 34;
    %sub;
    %ix/vec4/s 4;
    %assign/vec4/off/d v0x55d8db46f260_0, 4, 5;
    %load/vec4 v0x55d8db4bb0c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d8db4bb0c0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55d8db4a6d20;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d8db4bb400_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d8db4bb4a0_0, 0, 1;
